{
  "name": "functions",
  "version": "1.0.0",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "serve:v2": "npm run build && node lib/v2/server.js",
  "start:v2": "node lib/v2/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/v2/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
  "test:redis": "node test_redis.js",
    "deploy": "npm run build && firebase deploy --only functions",
    "deploy:v2": "npm run build && firebase deploy --only functions:apiv2"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "async-retry": "^1.3.3",
    "axios": "^1.3.4",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "firebase-admin": "^11.10.0",
    "firebase-functions": "^5.1.0",
    "gunzip": "^1.2.2",
    "gunzip-file": "^0.1.1",
    "p-retry": "^5.1.2",
    "redis": "^5.8.3",
    "retry": "^0.13.1",
    "swagger-ui-express": "^5.0.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "swagger-jsdoc": "^6.2.8",
  "tsconfig-paths": "^3.14.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.13",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.5",
    "@types/node-fetch": "^2.6.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/xml2js": "^0.4.11",
    "firebase-functions-test": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "private": true
}
