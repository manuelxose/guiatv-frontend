{
    "architect": {
        "build": {
            "builder": "@angular-devkit/build-angular:application",
            "options": {
                "outputPath": "dist/guiatv",
                "index": "src/index.html",
                "browser": "src/main.ts",
                "server": "src/main.server.ts",
                "polyfills": [
                    "zone.js"
                ],
                "tsConfig": "tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                    {
                        "glob": "**/*",
                        "input": "src/assets",
                        "output": "/assets"
                    },
                    {
                        "glob": "favicon.ico",
                        "input": "src",
                        "output": "/"
                    }
                ],
                "styles": [
                    "node_modules/bootstrap-icons/font/bootstrap-icons.css",
                    "node_modules/swiper/swiper.min.css",
                    "src/styles.scss"
                ],
                "scripts": [],
                "allowedCommonJsDependencies": [
                    "express"
                ],
                "ssr": {
                    "entry": "server.ts"
                }
            },
            "configurations": {
                "production": {
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "1MB",
                            "maximumError": "1.5MB"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "30kB",
                            "maximumError": "40kB"
                        },
                        {
                            "type": "bundle",
                            "maximumWarning": "1MB",
                            "maximumError": "1.5MB"
                        }
                    ],
                    "optimization": {
                        "scripts": true,
                        "styles": {
                            "minify": true,
                            "inlineCritical": false
                        },
                        "fonts": true
                    },
                    "outputHashing": "all",
                    "sourceMap": false,
                    "extractLicenses": true,
                    "aot": true
                },
                "development": {
                    "optimization": false,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "builder": "@angular-devkit/build-angular:dev-server",
            "configurations": {
                "production": {
                    "buildTarget": "guiatv:build:production"
                },
                "development": {
                    "buildTarget": "guiatv:build:development"
                }
            },
            "defaultConfiguration": "development"
        },
        "serve-ssr": {
            "builder": "@angular-devkit/build-angular:ssr-dev-server",
            "options": {
                "port": 4200
            },
            "configurations": {
                "development": {
                    "buildTarget": "guiatv:build:development"
                },
                "production": {
                    "buildTarget": "guiatv:build:production"
                }
            },
            "defaultConfiguration": "development"
        },
        "prerender": {
            "builder": "@angular-devkit/build-angular:prerender",
            "options": {
                "buildTarget": "guiatv:build"
            },
            "configurations": {
                "production": {
                    "buildTarget": "guiatv:build:production"
                },
                "development": {
                    "buildTarget": "guiatv:build:development"
                }
            },
            "defaultConfiguration": "production"
        },
        "extract-i18n": {
            "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
            "builder": "@angular-devkit/build-angular:karma",
            "options": {
                "polyfills": [
                    "zone.js",
                    "zone.js/testing"
                ],
                "tsConfig": "tsconfig.spec.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                    {
                        "glob": "**/*",
                        "input": "src/assets",
                        "output": "/assets"
                    },
                    {
                        "glob": "favicon.ico",
                        "input": "src",
                        "output": "/"
                    }
                ],
                "styles": [
                    "@angular/material/prebuilt-themes/azure-blue.css",
                    "src/styles.scss"
                ],
                "scripts": []
            }
        }
    }
}